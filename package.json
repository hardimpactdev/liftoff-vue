{
  "name": "@hardimpactdev/liftoff-vue",
  "description": "A Vue 3 component library for building clean, calm user interfaces. Built on Nuxt UI with layout, navigation, kanban, and chart components.",
  "author": "Hard Impact <hello@hardimpact.dev>",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui",
    "nuxt-ui",
    "tailwindcss",
    "laravel",
    "inertia",
    "kanban",
    "charts",
    "typescript"
  ],
  "homepage": "https://github.com/hardimpactdev/liftoff-vue#readme",
  "bugs": {
    "url": "https://github.com/hardimpactdev/liftoff-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hardimpactdev/liftoff-vue.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "include": [
    "src"
  ],
  "main": "dist/liftoff-vue.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/liftoff-vue.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite/defineLiftoffConfig.js",
      "types": "./dist/src/vite/defineLiftoffConfig.d.ts"
    }
  },
  "version": "0.1.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build && npm run copy-vite-plugin",
    "build-watch": "vue-tsc -b && vite build --watch",
    "preview": "vite preview",
    "storybook": "LAUNCH_EDITOR=cursor storybook dev -p 6006",
    "copy-vite-plugin": "mkdir -p dist/vite/ts && cp -r src/vite/*.ts dist/vite/ts/ && npx esbuild src/vite/*.ts --outdir=dist/vite --format=esm",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@inertiajs/core": "^2.3.6",
    "@inertiajs/vue3": "^2.0.0",
    "@nuxt/ui": "^4.3.0",
    "@tailwindcss/vite": "^4.0.0",
    "@tanstack/vue-table": "^8.21.3",
    "@types/node": "^25.0.3",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.1.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "laravel-vite-plugin": "^2.0.1",
    "laravel-vue-i18n": "^2.8.0",
    "lucide-vue-next": "^0.562.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vee-validate": "^4.15.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue": "^3.5.0",
    "vue-chartjs": "^5.3.3",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-themes": "^10.1.11",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/vue3-vite": "^10.1.11",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser": "^4.0.16",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/tsconfig": "^0.8.1",
    "glob": "^13.0.0",
    "playwright": "^1.57.0",
    "storybook": "^10.1.11",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-run": "^0.6.1",
    "vitest": "^4.0.16",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.6.4",
    "vue-tsc": "^3.2.1"
  },
  "peerDependencies": {
    "@inertiajs/vue3": "^2.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "laravel-vite-plugin": "^2.0.1",
    "laravel-vue-i18n": "^2.8.0",
    "vite-plugin-run": "^0.6.1",
    "vue": "^3.5.0"
  }
}
